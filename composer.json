{
    "name": "sebastian/de-legacy-fy",
    "description": "Tool for dealing with legacy PHP code",
    "homepage": "https://github.com/sebastianbergmann/de-legacy-fy",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sebastian Bergmann",
            "email": "sebastian@phpunit.de",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/sebastianbergmann/de-legacy-fy/issues"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "sebastian/version": "^2.0.1",
        "symfony/console": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0",
        "mikey179/vfsStream": "^1.6"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "bin": [
        "de-legacy-fy"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
